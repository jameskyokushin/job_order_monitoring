<div class="jo-link">
  <%= link_to "Add Inquiry ", new_inquiry_path %> |
  <%= link_to 'List Inquiry', inquiries_path %> |
  <%= link_to "Sign out",  destroy_user_session_path,  :method=> :delete %> 
</div>
<h1>Telephone Inquiry</h1>
<div class="email">
<%= current_user.email if user_signed_in? %>
</div>

<table>
  <tr>
    <th>Date</th>
    <th>Caller Name</th>
    <th>Company Name</th>
    <th>Landline</th>
    <th>Concern</th>
    <th>Request</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @inquiries.each do |inquiry| %>
  <tr>
    <td><%= inquiry.date %></td>
    <td><%= inquiry.client_name %></td>
    <td><%= inquiry.inq_company_name %></td>
    <td><%= inquiry.tel_cel %></td>
    <td><%= inquiry.concern %></td>
    <td><%= inquiry.request %></td>
    <td><%= link_to 'View', inquiry %></td>
    <!--<td><%= link_to 'Edit', edit_inquiry_path(inquiry) %></td>-->
    <!--<td><%= link_to 'Destroy', inquiry, :confirm => 'Are you sure?', :method => :delete %></td>-->
  </tr>
<% end %>
</table>

<br />


