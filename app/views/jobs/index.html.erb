<h1>Listing jobs</h1>

<table>
  <tr>
    <th>Date time started</th>
    <th>Ref no</th>
    <th>Company</th>
   
    <th>Technical</th>
    
   
    
    <th>Contact person</th>
   
   
    
    <th>Status</th>
    <th>Prepared by</th>
    
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @jobs.each do |job| %>
  <tr>
    <td><%= job.date_time_started %></td>
    <td><%= job.ref_no %></td>
    <td><%= job.company %></td>
   
    <td><%= job.technical %></td>
   
   
    <td><%= job.contact_person %></td>
    
    
    <td><%= job.status %></td>
    <td><%= job.prepared_by %></td>
   
    <td><%= link_to 'Show', job %></td>
    <td><%= link_to 'Edit', edit_job_path(job) %></td>
    <td><%= link_to 'Destroy', job, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Job', new_job_path %>
